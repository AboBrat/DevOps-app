@Library('my-sherd-lib')_

pipeline {
    agent any
    
    stages {
        stage('Checkout'){
            steps{
            gitCheckout(
                branch: "main",
                url: "https://github.com/AboBrat/DevOps-app.git"
            )
            }
        }
        stage('Unit Test maven'){
            steps{
            script{
                mvnTest()
                }
            }
        }
    }
}



